[zfs-testing]
name=ZFS on Linux for EL 6 - Testing
baseurl=http://archive.zfsonlinux.org/epel-testing/6/$basearch/
enabled=0
metadata_expire=7d
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-zfsonlinux
<% if @exclude.to_s != '' -%>
exclude=<%= @exclude.join(' ') %>
<% end -%>

[zfs-testing-source]
name=ZFS on Linux for EL 6 - Testing Source
baseurl=http://archive.zfsonlinux.org/epel-testing/6/SRPMS/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-zfsonlinux
priority=<%= @priority %>
<% if @exclude.to_s != '' -%>
exclude=<%= @exclude.join(' ') %>
<% end -%>
